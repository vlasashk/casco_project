
WindowManager:
	FirstWindow:
	SecondWindow:
	ThirdWindow:
	FourthWindow:
	FifthWindow:
	SixthWindow:
	SeventhWindow:

		

<FirstWindow>:
	name: "first"

	
	FloatLayout:
		size: root.width, root.height
		

		canvas.before:
			Rectangle:
				pos: self.pos
				size: self.size
				source:  "back/background_1.png"

		Label:
			text: "Choose your gender"
			size: self.texture_size
			halign: 'center'
            valign: 'middle'
			font_size: 50
			pos_hint:{"y":0.33}
			color: 0, 0, 0, 1


		Button:
			font_size: 32
			size_hint: 0.22, 0.27
			background_color: 0, 0.30, 0.59, 0
			pos_hint: {"x":0.18, "y":0.36}
			on_release: 
				app.pol = root.set_male()
				app.root.current = "second"
				root.manager.transition.direction = "left"
			Image:
				source:  "Images/man.png"
				size: self.parent.size
				center_x: self.parent.center_x
				center_y: self.parent.center_y

		Button:
			font_size: 32
			size_hint: 0.22, 0.27
			background_color: 0, 0.30, 0.59, 0
			pos_hint: {"x":0.62, "y":0.36}
			on_release: 
				app.pol = root.set_female()
				app.root.current = "second"
				root.manager.transition.direction = "left"
			Image:
				source:  "Images/woman.png"
				size: self.parent.size
				center_x: self.parent.center_x
				center_y: self.parent.center_y
				


<SecondWindow>:

	name: "second"
	vozrast: vozrast



    FloatLayout:
		size: root.width, root.height


		canvas.before:
			Rectangle:
				pos: self.pos
				size: self.size
				source:  "back/background_2.png"

		Label:
			text: "Enter your age"
			size: self.texture_size
			halign: 'center'
            valign: 'middle'
			font_size: 50
			pos_hint:{"y":0.33}
			color: 0, 0, 0, 1

		TextInput:
			id: vozrast
			text: "0"
			size_hint: 0.4, 0.4
			pos_hint: {"x":0.3, "y":0.3}
			halign: 'center'
            valign: 'middle'
			id: vozrast
			font_size: 190
			background_color: (0, 0.30, 0.59, 0)

		Button:
			font_size: 32
			size_hint: 0.1, 0.15
			background_color: 0, 0.30, 0.59, 0
			pos_hint: {"x":0.83, "y":0.1}
			
			on_release: 
				app.root.current = "third"
				root.manager.transition.direction = "left"
				app.vozrast = root.vozrast_read()
			Image:
				source:  "Images/right.png"
				size: self.parent.size
				center_x: self.parent.center_x
				center_y: self.parent.center_y
		Button:
			font_size: 32
			size_hint: 0.1, 0.15
			background_color: 0, 0.30, 0.59, 0
			pos_hint: {"x":0.07, "y":0.1}
			on_release: 
				app.root.current = "first"
				root.manager.transition.direction = "right"
				
			Image:
				source:  "Images/left.png"
				size: self.parent.size
				center_x: self.parent.center_x
				center_y: self.parent.center_y

